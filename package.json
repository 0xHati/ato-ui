		"tinycolor2": "^1.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.4",
		"unocss": "^0.51.13",
		"vite": "^4.3.7"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@unocss/autocomplete": "^0.51.13",
		"@unocss/core": "^0.51.13",
		"@unocss/extractor-svelte": "^0.51.13",
		"@unocss/preset-attributify": "^0.51.13",
		"@unocss/preset-uno": "^0.51.13",
		"@unocss/preset-wind": "^0.51.13",
